<launch>
    <arg name="dof" default="6"/>
    <arg name="robot_ip" default="192.168.1.203"/>

    <include file="$(find-pkg-share xarm_planner)/launch/xarm6_planner_realmove.launch.py">
        <arg name="robot_ip" value="$(var robot_ip)"/>
    </include>

    <node pkg="move_ptp_repeat" exec="move_ptp_repeat" name="move_ptp_repeat" output="screen">
        <param name="dof" value="$(var dof)"/>
    </node>
</launch>